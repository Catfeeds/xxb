<p id="notice"><%= notice %></p>

<h1>Listing Practice Parsers</h1>

<table>
  <thead>
    <tr>
      <th>Practice</th>
      <th>Word</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practice_parsers.each do |practice_parser| %>
      <tr>
        <td><%= practice_parser.practice %></td>
        <td><%= practice_parser.word %></td>
        <td><%= practice_parser.deleted_at %></td>
        <td><%= link_to 'Show', practice_parser %></td>
        <td><%= link_to 'Edit', edit_practice_parser_path(practice_parser) %></td>
        <td><%= link_to 'Destroy', practice_parser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Practice parser', new_practice_parser_path %>
