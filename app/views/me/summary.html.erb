
<div class="container col-xs-11 col-sm-10 col-md-8 affix">
  <div class="btn-toolbar pull-right" role="toolbar" arial-label="toolbar">
   <!-- 附加功能 -->
    <div class="btn-group" role="btn-group" arial-label="operation">
      <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
        <li>
          <%= link_to "账户信息", edit_registration_path(current_user) %>
        </li>
        <li role="separator" class="divider"></li>
        <li>
          <%= link_to "查看积分", me_point_card_path %>
        </li>
        <li>
          <%= link_to "查看做过的试题", evaluations_path %>
        </li>
        <li role="separator" class="divider"></li>
        <li>
         <%= link_to "我要评卷", me_justify_path %>
        </li>
        <li>
          <%= link_to "查看我评过的卷子", justices_path %>
        </li>
        <li role="separator" class="divider"></li>
        <li>
          <%= link_to "我布置的作业", me_assign_homeworks_path %>
        </li>
        <li>
          <%= link_to "我要完成的作业", me_my_homeworks_path %>
        </li>
        <li role="separator" class="divider"></li>
        <li>
          <%= link_to "我任教的班级", me_as_a_teacher_path %>
        </li>
        <li>
          <%= link_to "我是学生的班级", me_as_a_student_path %>
        </li>
      </ul>
    </div>
    <!-- 导航按钮 -->
    <div class="btn-group" role="btn-group" arial-label="navigation">
      <%= link_to t('.up', :default => t("helpers.links.up")), "/", class: 'btn btn-default', "data-toggle": "tooltip", "data-placement": "bottom", title: t('.up', default: t("helpers.titles.up"), :model => t("activerecord.models.site")) %>
      <%= link_to t('.retreat', :default => t("helpers.links.retreat")), :back, :class => 'btn btn-default', "data-toggle": "tooltip", "data-placement": "bottom", title: t('.retreat', :default => [:"helpers.titles.retreat"]) %>
    </div>
  </div>
</div>


